{
  "name": "@ts-to-json-schema/transform",
  "version": "1.0.1",
  "description": "Type transformer for TypeScript to JSON Schema",
  "author": "Pedro Marcelo <pedromarcelodesaalves@gmail.com>",
  "homepage": "https://github.com/pedromdev/ts-to-json-schema",
  "bugs": "https://github.com/pedromdev/ts-to-json-schema/issues",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
